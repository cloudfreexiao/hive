--工程名字
PROJECT_NAME = "luabus"

--目标名字
TARGET_NAME = "luabus"

----工程类型: static/dynamic/exe
PROJECT_TYPE = "static"

--需要定义的选项
DEFINES = {
	"FMT_HEADER_ONLY"
}

--需要的include目录
INCLUDES = {
	"../luna/src",
	"../../extend/lua/lua",
    "../../extend/fmt/include",
	"../../extend/utility"
}
---子目录路径

--自动搜索子目录
AUTO_SUB_DIR = true

--目标文件前缀
LIB_PREFIX = 1

--依赖项目
DEPS = {
	"luna"
}

--分组定义
GROUP = "core"